# Punch Configuration
# Copy this to .env and fill in your values

# Telegram Bot (get token from @BotFather, user ID from @userinfobot)
# PUNCH_TELEGRAM_TOKEN=your-bot-token-here
# PUNCH_TELEGRAM_USERS=123456789

# Web Dashboard
PUNCH_WEB_PORT=8080

# Claude Code
PUNCH_MAX_CONCURRENT=4

# Logging
PUNCH_LOG_LEVEL=INFO
